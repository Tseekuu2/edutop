@charset "utf-8";
html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, embed, 
figure, figcaption, footer, header, hgroup, 
menu, nav, output, ruby, section, summary,
time, mark, audio, video, input, button {
	margin: 0;
	padding: 0;
	border: 0;
	font-size: 100%;
    font-style: normal;
    font-weight: 400;
	vertical-align: middle;
    text-decoration:none;
    word-break:break-all; word-wrap:break-word;
    font-family: 'Nanum Gothic';
}
article, aside, details, figcaption, figure, 
footer, header, hgroup, menu, nav, section {
	display: block;
}
body {
	line-height: 1.1;
}
ol, ul {
	list-style: none;
}
blockquote, q {
	quotes: none;
}
blockquote:before, blockquote:after,
q:before, q:after {
	content: '';
	content: none;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
}
button{cursor: pointer;}

/* s_font */
@font-face {
  font-family: 'Nanum Gothic';
  font-style: normal;
  font-weight: 400;
  src: url(fonts/NanumGothic-Regular.eot);
  src: url(fonts/NanumGothic-Regular.eot?#iefix) format('embedded-opentype'),
       url(fonts/NanumGothic-Regular.woff2) format('woff2'),
       url(fonts/NanumGothic-Regular.woff) format('woff');
}

/* s_noscript  body wrap */
noscript{width:0px; height:0px; visibility:hidden; text-indent:-100000px; font-size:0px; line-height:0px;}
body{background: rgba(255, 255, 255, 1); }
#wrap{display:table;width: 1100px;margin:0 auto;height:auto;background: rgba(255, 255, 255, 1);}
#wrap .container{display:table;width: 1000px;margin: 0 50px;height:auto;padding:0;}
/* e_noscript  body wrap */

/* header */
div.header{display:table; width:100%; height:auto; border-top:solid 1px rgb(216, 216, 216); border-bottom:0px;background:rgba(255, 255, 255, 1);}
div.header .logo-div{display:table;width: 1100px;margin: 40px auto 66px auto;padding: 0;}
div.header .logo-div img.logo{display:table;width: 136px;height: 50px;vertical-align:middle;margin:0 704px 65px 260px;padding: 0;float: left;}
div.header .logo-div img.logo_u{display:table;width: 400px;height: auto;vertical-align:middle;margin: 0 auto 65px auto;padding: 0;}
div.header .logo-div h1{display:table;width: 1000px;height: auto;line-height: 55px;padding: 0;margin: 0 50px;text-align:center;font-weight: 300;letter-spacing: 0px;color: rgba(255, 255, 255, 0);background: url(../img/osce_tt1.png) 49% 49% no-repeat;}
/* e_header */

/* s_lD PW */
.container div.formwrap{display:table;margin: 0 auto 100px auto;padding: 40px 46px;border:solid 1px rgb(42, 104, 179);}
.container .form_contents{display:table;margin:0;padding: 0;width: 100%;height:auto;}
.container .form_contents h3{display: none;line-height:30px;text-align:center;font-size: 18px;padding: 5px 15px 1px 15px;margin: 0;border-bottom:solid 1px rgb(168, 222, 255);border-top:solid 1px rgb(168, 222, 255);color: rgb(88, 165, 247);word-spacing:0px;letter-spacing: 0px;width:450px;float:right;background: rgb(249, 253, 255);}
.container .form_contents h3 span{font-size: 17.5px;}
.container .form_contents .login-form{display:table;margin:0;padding:0;width:auto;}
.form.login-form .label-check{display:inline-block;padding-left:28px;padding-top:2px;padding-bottom:3px;margin-bottom:10px;}
.form.login-form label.r-on { background: url(../img/radio-on.png) no-repeat;}
.check-member-div .oper{color:#1268b2;}
.container .form_contents .login-form .inputbutton-div{background:#fff;padding: 0;margin: 0;}
.container .form_contents .login-form .section-radio-div{margin-bottom:5px;}
.container .form_contents .login-form .section-radio-div span{display:inline-block;width:89px;}
.container .form_contents .login-form .section-radio-div label{padding-left:20px;margin-right:20px;background-position:left center;cursor:pointer;}
.container .form_contents .login-form .inputbutton-div .login-input-div{width:auto; display:inline-block; float:left; margin: 0; }
.container .form_contents .login-form .inputbutton-div .login-input-div p{display: table; margin:4px 0 6px 0;}
.container .form_contents .login-form .inputbutton-div .login-input-div p input{border: solid 1px rgba(140, 200, 250, 1);margin: 0 5px 0 0;padding:0 10px;line-height:26px;height: 45px;width: 320px;vertical-align:middle;font-size: 16px;color: rgba(42, 136, 215, 1);letter-spacing: 1px;float: left;}
.container .form_contents .login-form .inputbutton-div .login-input-div p label{display:inline-block;width:115px;font-size: 14px;cursor:pointer;color: rgb(42, 125, 214);background: rgb(228, 245, 255);padding: 2.2px 0 0 0;margin: 0 0 0 0;line-height: 26px;text-align: left;text-indent: 21px;float: left;}
.container .form_contents .login-form .inputbutton-div .login-button-div{width: 130px;display:inline-block;vertical-align:top;}
.container .form_contents .login-form .inputbutton-div .login-button-div button{display:inline-block;width:100%;height: 100px;cursor:pointer;font-size: 23px;border-top-right-radius: 33px;margin: 4px 0 0 0;letter-spacing: 0px;background: rgba(42, 104, 179, 1);border: solid 1px rgba(42, 104, 179, 1);color:rgba(255, 255, 255, 1);}
.container .form_contents .login-form .inputbutton-div .login-button-div button:hover{background: rgba(238, 249, 255, .6);color: rgba(58, 160, 246, 1);border: solid 1px rgba(140, 200, 250, 1); }
.lb-checkbox.on{background-position:left center;color:#111;}
.lb-checkbox{padding-left:20px;background-position:left center;}
.container .form_contents .login-form .login-chceck-div{display: block; padding:0; margin: 0 0 17px 77px;}
.container .form_contents .login-form .login-chceck-div p.p1{display:inline-block; width:40%; line-height: 33px; font-size:14px; text-align: center; background: rgb(128, 201, 255); color: rgb(255, 255, 255); padding:0; margin: 0; border-top-left-radius:17px; border:solid 1px rgb(88, 165, 247); border-bottom-left-radius:17px; }
.container .form_contents .login-form .login-chceck-div p.p1:hover{background: rgb(228, 245, 255); color:rgb(88, 165, 247); border:solid 1px rgb(88, 165, 247);}
.container .form_contents .login-form .login-chceck-div p.p2{display:inline-block; width:40%; line-height: 33px; font-size:14px; text-align: center; background: rgb(88, 165, 247); color: rgb(255, 255, 255); padding:0; margin: 0; border-top-right-radius:17px; border:solid 1px rgb(88, 165, 247); border-bottom-right-radius:17px; }
.container .form_contents .login-form .login-chceck-div p.p2:hover{background: rgb(228, 245, 255); color:rgb(88, 165, 247); border:solid 1px rgb(88, 165, 247);}
.container .form_contents .login-form .login-chceck-div p label{display: inline-block; width:100%; cursor:pointer;}
.container .form_contents .login-form .login-chceck-div p label input.check-id{display: inline-block; cursor:pointer; width:19px; height:19px; margin: 0 5px 0 -40px; }
.container .form_contents .login-form .login-chceck-div p a{display: inline-block; width:100%; color:rgb(255, 255, 255); cursor:pointer;}
.container .form_contents .login-form .login-chceck-div p a:hover{color:rgb(88, 165, 247);}
.container .form_contents .login-form .login-chceck-div p input{vertical-align:middle;}
/* e_lD PW */

/* s_footer */
#footer{display: table; margin:0 auto; border-top:solid 1px rgb(190, 230, 253); width:100%; height:60px; background:none;}
#footer .footer_contents{display: table;margin:0 auto;width: 1000px;height:auto;padding: 17px 50px;}
#footer .footer_contents img{display:inline-block; width:100px;height:40px; margin:0; float:left;}
#footer .footer_address{display: table;width:auto;margin: 0 auto;}
#footer .footer_address p{font-size:12px;margin:0;padding:4px 0 0 0;color: #929292;letter-spacing: 1px;}
#footer p.guide_call{display:table;width: auto;height: 20px;line-height: 20px;margin:0;padding:11px 0 9px 0;float:right;text-align:center;font-size: 15px;letter-spacing: -1px;color: rgba(77, 148, 208, 1);box-sizing: content-box;}
#footer p.guide_call i.phone{position:relative;top: 0;display:inline-block;font-size: 17px;margin: 0 0 0 5px;color:rgb(123, 205, 255);}
#footer p.guide_call span.call{display:inline-block;font-size:15px;margin: 0 5px 0 4px;color:rgb(23, 100, 167);}
#footer p.guide_call span.con{position:relative; top: -1.5px; display:inline-block; font-size:12px; margin:0 2px 0 0; color:rgb(144, 144, 144); letter-spacing:-1px; }
/* e_footer */


